<%= form_with model: @job_opportunity, local: true do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :skills %>
  <%= f.text_field :skills %>
  <%= f.label :salary %>
  <%= f.number_field :salary %>
  <%= f.label :job_level %>
  <%= f.text_field :job_level %>
  <%= f.label :end_data %>
  <%= f.date_field :end_data %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.collection_select :headhunter_id, @headhunters, :id, :email %>
  <%= f.label :headhunter_id %>
  <%= f.submit 'Enviar' %>
<% end %>